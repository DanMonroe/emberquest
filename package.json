{
  "name": "emberquest",
  "version": "0.9.0",
  "private": true,
  "description": "Small description for emberquest goes here",
  "repository": "",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "FASTBOOT_DISABLED=true ember serve --port 4400",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "audio": "cd emberquest_work/sprites_audio && audiosprite --output eq_audio --path '/audio/sounds'  audio/*.mp3 audio/*.wav audio/*.ogg && cp eq_audio.* ../../public/audio/sounds && cd -",
    "music": "cd emberquest_work/sprites_music && audiosprite --output eq_music --path '/audio/music'  music/*.mp3 music/*.wav music/*.ogg && cp eq_music.* ../../public/audio/music && cd -"
  },
